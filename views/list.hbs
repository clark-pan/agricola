<body>
	{{> user}}
	{{> flash}}
	{{#if rooms}}
	<ul>
		{{#forEach rooms}}
		<li><a href="/room/{{this._id}}">{{this._id}}</a></li>
		{{/forEach}}
	</ul>
	{{else}}
	There are currently no rooms<br />
	<form action="/room/new" method="post">
		<input type="submit" value="Create new room" />
	</form>
	{{/if}}
</body>
